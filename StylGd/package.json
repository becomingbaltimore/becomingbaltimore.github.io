{
  "name": "becoming-baltimore",
  "version": "0.0.1",
  "description": "Custom living style guide approach to developing the Becoming Baltimore site.",
  "main": "index.html",
  "scripts": {
    "css": "cssnext src/build.css _public/css/index.css",
    "minify:css": "cssnext -c src/build.css _public/css/index.min.css",
    "watch:css": "watch 'npm run css && npm run minify:css' ./src",
    "serve": "http-server ./_public/",
    "start": "npm run watch:css & npm run serve"
  },
  "keywords": [
    "css4"
  ],
  "author": "bzelip",
  "license": "GPL-3.0",
  "dependencies": {
    "basscss-background-colors": "^1.1.1",
    "basscss-background-images": "0.0.9",
    "basscss-base-typography": "^2.0.1",
    "basscss-border-colors": "^1.1.1",
    "basscss-borders": "^2.0.4",
    "basscss-colors": "^2.0.0",
    "basscss-defaults": "^2.1.1",
    "basscss-grid": "^1.0.3",
    "basscss-media-object": "^1.0.4",
    "basscss-positions": "^1.0.4",
    "basscss-responsive-states": "^1.0.3",
    "basscss-responsive-white-space": "0.0.1",
    "basscss-table-object": "0.0.7",
    "basscss-type-scale": "^1.0.0",
    "basscss-ui-utilities": "0.0.3",
    "basscss-utilities": "0.0.21",
    "basscss-utility-headings": "0.0.6",
    "basscss-utility-layout": "^2.0.3",
    "basscss-utility-responsive-states": "0.0.5",
    "basscss-utility-typography": "^2.0.0",
    "basscss-utility-white-space": "0.0.7",
    "basscss-white-space": "^1.1.2",
    "flex-object": "^2.0.2"
  },
  "devDependencies": {
    "cssnext": "^1.8.2",
    "http-server": "^0.8.0",
    "watch": "^0.16.0"
  }
}
